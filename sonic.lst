000012B6 5147                       		subq.w	#8,d7	; get shift value
000012B8 3205                       		move.w	d5,d1
000012BA EE69                       		lsr.w	d7,d1	; shift so that high bit of the code is in bit position 7
000012BC 0C01 00FC                  		cmpi.b	#%11111100,d1	; are the high 6 bits set?
000012C0 6400                       		bcc.s	NemPCD_InlineData	; if they are, it signifies inline data
000012C2 0241 00FF                  		andi.w	#$FF,d1
000012C6 D241                       		add.w	d1,d1
000012C8 1031 1000                  		move.b	(a1,d1.w),d0	; get the length of the code in bits
000012CC 4880                       		ext.w	d0
000012CE 9C40                       		sub.w	d0,d6	; subtract from shift value so that the next code is read next time around
000012D0 0C46 0009                  		cmpi.w	#9,d6	; does a new byte need to be read?
000012D4 6400                       		bcc.s	loc_14B2	; if not, branch
000012D6 5046                       		addq.w	#8,d6
000012D8 E145                       		asl.w	#8,d5
