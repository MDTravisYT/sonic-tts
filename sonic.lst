000012F2 6600                       		bne.s	NemPCD_WritePixel_Loop	; if not, loop
000012F4 4ED3                       		jmp	(a3)	; otherwise, write the row to its destination, by doing a dynamic jump to NemPCD_WriteRowToVDP, NemDec_WriteAndAdvance, NemPCD_WriteRowToVDP_XOR, or NemDec_WriteAndAdvance_XOR
