00001358                            ; Part of the Nemesis decompressor, builds the code table (in RAM)
00001358                            ; ---------------------------------------------------------------------------
00001358                            
00001358                            
00001358                            NemDec_BuildCodeTable:
00001358 1018                       		move.b	(a0)+,d0	; read first byte
0000135A                            
0000135A                            NemBCT_ChkEnd:
0000135A 0C00 00FF                  		cmpi.b	#$FF,d0	; has the end of the code table description been reached?
0000135E 6600                       		bne.s	NemBCT_NewPALIndex	; if not, branch
00001360 4E75                       		rts	; otherwise, this subroutine's work is done
00001362                            ; ===========================================================================
00001362                            
00001362                            NemBCT_NewPALIndex:
00001362 3E00                       		move.w	d0,d7
00001364                            
00001364                            NemBCT_Loop:
00001364 1018                       		move.b	(a0)+,d0	; read next byte
00001366 0C00 0080                  		cmpi.b	#$80,d0	; sign bit being set signifies a new palette index
0000136A 64EE                       		bcc.s	NemBCT_ChkEnd	; a bmi could have been used instead of a compare and bcc
0000136C                            		
0000136C 1200                       		move.b	d0,d1
0000136E 0247 000F                  		andi.w	#$F,d7	; get palette index
00001372 0241 0070                  		andi.w	#$70,d1	; get repeat count for palette index
00001376 8E41                       		or.w	d1,d7	; combine the two
00001378 0240 000F                  		andi.w	#$F,d0	; get the length of the code in bits
0000137C 1200                       		move.b	d0,d1
0000137E E149                       		lsl.w	#8,d1
00001380 8E41                       		or.w	d1,d7	; combine with palette index and repeat count to form code table entry
00001382 7208                       		moveq	#8,d1
00001384 9240                       		sub.w	d0,d1	; is the code 8 bits long?
00001386 6600                       		bne.s	NemBCT_ShortCode	; if not, a bit of extra processing is needed
00001388 1018                       		move.b	(a0)+,d0	; get code
0000138A D040                       		add.w	d0,d0	; each code gets a word-sized entry in the table
0000138C 3387 0000                  		move.w	d7,(a1,d0.w)	; store the entry for the code
00001390 60D2                       		bra.s	NemBCT_Loop	; repeat
00001392                            ; ===========================================================================
00001392                            
00001392                            ; the Nemesis decompressor uses prefix-free codes (no valid code is a prefix of a longer code)
00001392                            ; e.g. if 10 is a valid 2-bit code, 110 is a valid 3-bit code but 100 isn't
00001392                            ; also, when the actual compressed data is processed the high bit of each code is in bit position 7
00001392                            ; so the code needs to be bit-shifted appropriately over here before being used as a code table index
00001392                            ; additionally, the code needs multiple entries in the table because no masking is done during compressed data processing
00001392                            ; so if 11000 is a valid code then all indices of the form 11000XXX need to have the same entry
00001392                            NemBCT_ShortCode:
00001392 1018                       		move.b	(a0)+,d0	; get code
00001394 D040                       		add.w	d0,d0	; shift so that high bit is in bit position 7
00001396 E368                       		lsl.w	d1,d0	; get index into code table
00001398 7A01                       		moveq	#1,d5
0000139A E36D                       		lsl.w	d1,d5
0000139C 5345                       		subq.w	#1,d5	; d5 = 2^d1 - 1
0000139E                            
0000139E                            NemBCT_ShortCode_Loop:
0000139E 3387 0000                  		move.w	d7,(a1,d0.w)	; store entry
000013A2 5440                       		addq.w	#2,d0	; increment index
000013A4 51CD FFF8                  		dbf	d5,NemBCT_ShortCode_Loop	; repeat for required number of entries
000013A8 60BA                       		bra.s	NemBCT_Loop
